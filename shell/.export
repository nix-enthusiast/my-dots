#--------------------MY BASH/ZSH ALIASES--------------------#

#--------EXPORTS--------#
## PATHS
export PATH="/usr/local/bin:$HOME/.local/usr/bin:$PATH:$HOME/.cargo/bin:$HOME/.go/bin/:/opt/homebrew/opt/rustup/bin"
export GOPATH="$HOME/.go"
export C_INCLUDE_PATH="$HOME/.local/usr/lib/c"
export C_PLUS_PATH="$HOME/.local/usr/lib/cxx"
export LD_LIBRARY_PATH="$HOME/.local/usr/lib/shared"
export STATIC_LIBRARY_PATH="$HOME/.local/usr/lib/static/"

## Other exports
export SHELL_SESSIONS_DISABLE=1
export EDITOR="nvim"
export FETCH="pfetch"
export SHELL_SESSIONS_DISABLE=1
export TERM='xterm-256color'
export COLORTERM="truecolor"

